{
    "name": "themonkeys/cachebuster",
    "description": "Adds MD5 hashes to the URLs of your application's assets, so when they change, their URL changes.",
    "homepage": "http://github.com/TheMonkeys/laravel-cachebuster",
    "keywords": ["assets", "cachebuster", "cdn", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "themonkeys",
            "email": "developers@themonkeys.com.au"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*|5.*",
        "symfony/http-kernel": "2.*|3.*|4.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Themonkeys\\Cachebuster": "src/"
        }
    },
    "minimum-stability": "dev"
}
